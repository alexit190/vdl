<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="8.0" stylesheets="@../../styles.css" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="historyTableView" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="titleTableColumn" minWidth="30.0" prefWidth="75.0" text="%stage.history.table.header.title" />
            <TableColumn fx:id="urlTableColumn" minWidth="30.0" prefWidth="75.0" text="%stage.history.table.header.url" />
            <TableColumn fx:id="locationTableColumn" minWidth="30.0" prefWidth="75.0" text="%stage.history.table.header.location" />
            <TableColumn fx:id="dtmTableColumn" minWidth="30.0" prefWidth="75.0" text="%stage.history.table.header.dtm" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT" spacing="4.0">
         <children>
            <Label fx:id="entriesNumberLabel" text="%stage.history.entries.number.label" HBox.hgrow="NEVER" />
            <ComboBox fx:id="entriesNumberComboBox" />
            <StackPane alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="clearHistoryBtn" mnemonicParsing="false" text="%stage.history.btn.clear" />
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</fx:root>
